<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.suchteam.opendata.SubscribeAssertion" table="subscribe_assertion">
        <id name="subscribeAssertionId" column="subscribe_assertion_id">
            <generator class="native" />
        </id>
        <property name="value" column="value"/>
       <many-to-one class="com.suchteam.opendata.Subscribe" name="subscribe" column="subscribe_id"/>
       <many-to-one class="com.suchteam.opendata.DatasetRecord" name="datasetRecord" column="dataset_record"/>
      
      </class>

</hibernate-mapping>