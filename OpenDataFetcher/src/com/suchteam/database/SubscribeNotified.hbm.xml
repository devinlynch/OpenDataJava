<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.suchteam.database.SubscribeNotified" table="subscribe_notified">
        <id name="subscribeNotifiedId" column="subscribe_notified_id">
            <generator class="native" />
        </id>
       <property name="notificationDate" column="notification_date"/>
       <property name="content" />
       <property name="subject" />
       <property name="sent" />
       <many-to-one class="com.suchteam.database.Subscribe" name="subscribe" column="subscribe_id"/>
       <many-to-one class="com.suchteam.database.DatasetRecord" name="datasetRecord" column="dataset_record_id"/>
      
      </class>

</hibernate-mapping>