<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.suchteam.database.DatasetInput" table="dataset_input">
        <id name="datasetInputId" column="dataset_input_id">
            <generator class="native" />
        </id>
        <property name="name" column="name"/>
        <property name="visualName" column="visual_name"></property>
        <many-to-one class="com.suchteam.database.DataType" name="dataType" column="data_type_id"/>
		<many-to-one class="com.suchteam.database.Dataset" name="dataset" column="dataset_id"/>  

    </class>
</hibernate-mapping>